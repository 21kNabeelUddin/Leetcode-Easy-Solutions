class Solution {
public:
    int hammingWeight(int n) {
        int count=0;
        bitset<32> binary(n);
        cout<<""<<binary;

         for (int i = 0; i < 32; i++) {
            if (binary[i] == 1) { // Check if the bit is 1
                count++;
            }
        }

        return count;
    }
};
